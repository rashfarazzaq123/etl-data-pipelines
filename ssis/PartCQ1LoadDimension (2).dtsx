              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[datawarehouse].[F_Purchase]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH po AS (
    SELECT 
        pol.po_line_id,
        po_hdr.po_no,
        po_hdr.order_date,
        pol.due_date,
        po_hdr.vendor_id,
        po_hdr.employee_id AS buyer_id,
        po_hdr.location_id,
        pol.product_id,
        pol.quantity,
        pol.unit_price
    FROM company.ABCCompany.PurchaseOrder po_hdr
    INNER JOIN company.ABCCompany.PurchaseOrderLine pol
        ON po_hdr.po_id = pol.po_id
),
contract_price AS (
    SELECT
        cl.product_id,
        c.vendor_id,
        cl.unit_price AS contract_unit_price,
        c.start_date,
        c.end_date
    FROM company.ABCCompany.Contract c
    INNER JOIN company.ABCCompany.ContractLine cl
        ON c.contract_id = cl.contract_id
),
approved_vendor AS (
    SELECT
        vendor_id,
        product_id,
        approved_from,
        approved_to
    FROM company.ABCCompany.VendorApproved
)
SELECT
    po.po_no,
    po.po_line_id,
    po.order_date,
    po.due_date,
    po.vendor_id,
    po.buyer_id,
    po.location_id,
    po.product_id,
    po.quantity AS ordered_qty,
    po.unit_price AS order_unit_price,
    cp.contract_unit_price,
    CASE WHEN av.vendor_id IS NOT NULL THEN 1 ELSE 0 END AS is_approved_vendor,
    CASE WHEN av.vendor_id IS NULL THEN 1 ELSE 0 END AS is_maverick
FROM po
LEFT JOIN contract_price cp
    ON cp.product_id = po.product_id
    AND cp.vendor_id = po.vendor_id
    AND po.order_date BETWEEN cp.start_date AND ISNULL(cp.end_date, '9999-12-31')
LEFT JOIN approved_vendor av
    ON av.vendor_id = po.vendor_id
    AND av.product_id = po.product_id
    AND po.order_date BETWEEN av.approved_from AND ISNULL(av.approved_to, '9999-12-31');</property>
